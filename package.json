{
  "name": "@bestdoctor/icons",
  "version": "2.0.0-beta.2",
  "description": "BestDoctor icons package",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "svgr -d ./src/icons ./icons-sources",
    "build": "webpack --config ./webpack.prod.js",
    "lint": "eslint --ext .ts,.tsx src",
    "format": "prettier --write 'src/**/*'",
    "typecheck": "yarn tsc --noEmit",
    "makecheck": "yarn typecheck && yarn lint",
    "release": "standard-version --no-verify"
  },
  "author": "Valery Pavlov",
  "license": "MIT",
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@types/react": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.15.2",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tsdoc": "^0.2.11",
    "husky": "4",
    "lint-staged": "^10.5.4",
    "node-sass": "^6.0.1",
    "prettier": "^2.2.1",
    "standard-version": "^9.3.2",
    "ts-loader": "^7.0.4",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslib": "^2.1.0",
    "typescript": "~4.1.5",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --no-stash"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/best-doctor/react-icons.git"
  },
  "bugs": {
    "url": "https://github.com/best-doctor/react-icons/issues"
  },
  "homepage": "https://github.com/best-doctor/react-icons#readme",
  "prettier": {
    "parser": "typescript",
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    }
  }
}
